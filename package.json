{
  "name": "LP",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:assets": "webpack --mode=development --watch",
    "dev:site": "eleventy --serve",
    "build": "cross-env NODE_ENV=production run-s clean build:assets build:site",
    "build:assets": "webpack --mode=production",
    "build:site": "eleventy",
    "clean": "rm -rf ./docs",
    "start": "webpack-dev-server --mode=development"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.2",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "script-loader": "^0.7.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "alpinejs": "^2.5.0",
    "animejs": "^3.2.0",
    "swiper": "^6.5.1",
    "tailwindcss": "^1.4.6",
    "tippy.js": "^6.2.5"
  }
}
